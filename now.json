{
  "version": 2,
  "alias": "simplepacecalc.com",
  "routes": [
    {
      "src": "/(.*)",
      "status": 301,
      "headers": { "Location": "https://simplepacecalc.com/$1" }
    }
  ],
  "name": "simple-pace-calc",
    "builds": [
      { "src": "*.html", "use": "@now/static" },
      { "src": "/assets/js/*.js", "use": "@now/static" },
      { "src": "/assets/css/*.css", "use": "@now/static" }
    ]
}